{
  "name": "golang-relay-starter-kit",
  "private": true,
  "description": "A quick way to get up and running with Relay on Golang",
  "repository": "github.com/sogko/golang-relay-starter-kit",
  "version": "0.1.0",
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production webpack",
    "dev": "NODE_ENV=development node dev-server.js",
    "start": "go run graphql.go & babel-node ./server.js",
    "update-schema": "cd ./scripts && go run updateSchema.go"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-relay-plugin": "0.10.0",
    "classnames": "^2.1.3",
    "express": "^4.13.1",
    "file-loader": "^0.11.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-relay": "^0.10.0",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^1.10.1"
  }
}
